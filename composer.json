{
  "name": "jurgenromeijn/wordpress-plugin-projects-rest",
  "homepage": "https://github.com/jurgenromeijn/wordpress-plugin-projects-rest",
  "description": "Description: A WordPress plugin that creates the rest interface for the project post type.",
  "license": "GPL-3.0",
  "type": "wordpress-plugin",
  "keywords": [
    "WordPress", "plugins", "themes", "php", "projects", "rest"
  ],
  "authors": [
    {
      "name": "Jurgen Romeijn",
      "email": "jurgen.romeijn@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0",
    "johnpbloch/wordpress": "~4.5",
    "wpackagist-plugin/rest-api": "~2.0",
    "symfony/dependency-injection": "~2.8",
    "symfony/config": "~2.8",
    "symfony/yaml": "~2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {"JurgenRomeijn\\ProjectsRest\\": "src/"}
  },
  "extra": {
    "wordpress-install-dir": "wp"
  }
}
